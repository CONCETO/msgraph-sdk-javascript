{
	"compilerOptions": {
		"importHelpers": true,
		"noEmitOnError": true,
		"noImplicitAny": false,
		"moduleResolution": "node",
		"removeComments": false,
		"sourceMap": true,
		"declaration": true
	},
	"ts-node": {
		"ignore": ["*.d.ts"],
		"compilerOptions": {
			"target": "es6",
			"module": "ES6"
		}
	}
}
